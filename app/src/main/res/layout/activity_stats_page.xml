<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/black"
    tools:context=".StatsPage">


    <ImageView
        android:id="@+id/zed"
        android:layout_width="63dp"
        android:layout_height="64dp"
        android:layout_marginTop="76dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.083"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/zeds" />

    <ImageView
        android:id="@+id/anivia"
        android:layout_width="63dp"
        android:layout_height="62dp"
        android:layout_marginTop="76dp"
        android:layout_marginEnd="104dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.131"
        app:layout_constraintStart_toEndOf="@+id/zed"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/anivia" />

    <TextView
        android:id="@+id/editTextTextPersonName"
        android:layout_width="392dp"
        android:layout_height="64dp"
        android:layout_marginTop="36dp"
        android:ems="10"
        android:fontFamily="@font/fq_bold"
        android:inputType="textPersonName"
        android:text="SELECT THE CHAMPION YOU'RE PLAYING AGAINST:"
        android:textAlignment="center"
        android:textColor="@color/white"
        android:textSize="13sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.473"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>